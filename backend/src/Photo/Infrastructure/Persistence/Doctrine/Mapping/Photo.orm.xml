<doctrine-mapping>
    <entity name="LaSalle\Performance\Photo\Domain\Aggregate\Photo" table="photo">
        <id name="id" type="uuid" />
        <field name="description" type="string" nullable="true"/>
        <field name="nameURL" type="string" nullable="false"/>
        <field name="tags" type="array" nullable="true"/>
        <field name="filter" type="filter" nullable="false"/>
        <field name="filtersToApply" type="filtersToApply" nullable="true"/>
        <field name="createdAt" type="datetime_immutable" nullable="false"/>
    </entity>
</doctrine-mapping>
